<html lang="en">
  <head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>NavegaPara</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <script src="captiveportal-jquery-latest.min.js"></script>
    <script src="captiveportal-bootstrap.min.js"></script>
    <script src="captiveportal-bootstrap.js"></script>
    <link href="captiveportal-bootstrap.css" rel="stylesheet">
    <link href="captiveportal-bootstrap-responsive.css" rel="stylesheet">
    <link href="captiveportal-captive.css" rel="stylesheet">
    <!-- CaptivePortal Login - versao 6.2 de 22/01/2015 -->
  </head>

  <body>
    <!-- Modal de Aviso -->
    <div class="modal hide fade" id="mAviso">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3 id="mAvTitulo"></h3>
      </div>
      <div class="modal-body" id="mAvContent">
      </div>
      <div class="modal-footer">
        <a href="#" class="btn btn-primary" data-dismiss="modal" id="mAvBotao"></a>
      </div>
    </div>
    <!-- FIM da Modal de Aviso -->

    <div class="container">
      <form class="form-signin" id="form-signin" name="form-signin" method="post" action="http://172.16.0.18:8002/index.php?zone=wifi_prod">
      	<img src="captiveportal-bann-navegapara.png" style="float:right">
        <h3 style="line-height: 10px;color:#999">Hotzone</h3>
        <h4 style="color:#158799" id="divRede"></h4>
        <div id="divForm">
        </div>  <!-- /divForm -->
	  <div id="divVersao"> CapPortal versao</div>
      </form>
    </div> <!-- /container -->
	<script type="text/javascript">
		$('#auth_user').keyup(function(){
				this.value = this.value.toLowerCase();
		});
		$("#form-signin").submit(function(e) {
			if ($.trim($('#auth_user').val()).length == 0 || $.trim($('#auth_pass').val()).length == 0){
				alert('Por favor, preencha o EMAIL e SENHA!');
				e.preventDefault();
			}
			$('#auth_user').val (function () { return this.value.toLowerCase();	});
			//$('#redirurl').val (function () { return 'http://captiveportal.prodepa.gov.br/captiveportal/login/mensagem.php?id='+document.getElementById("auth_user").value+'&url='+document.getElementById("urlDtn").value});
			$.post( "http://captiveportal.prodepa.gov.br/captiveportal/login/load_prelogin.php", {email: $('#auth_user').val(), senha: $('#auth_pass').val(), urlDtn: $('#urlDtn').val()}, function( data ) {});
		});
	</script>
  <script src="http://captiveportal.prodepa.gov.br/captiveportal/login/load_data.php?page=login&url=http://downloads.arduino.cc/libraries/library_index.json"></script>
  </body>
</html>
